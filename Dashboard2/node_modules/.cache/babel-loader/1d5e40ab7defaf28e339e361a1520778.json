{"ast":null,"code":"import CrudComment from '../../components/CrudComment.vue';\nimport Like from '../../components/Like.vue';\n\nconst axios = require('axios');\n\nexport default {\n  name: 'CrudDetailPost',\n  components: {\n    CrudComment,\n    Like\n  },\n  props: ['id'],\n\n  data() {\n    return {\n      post: {}\n    };\n  },\n\n  mounted() {\n    axios.get(`http://localhost:8000/api/posts/` + this.id, {}).then(response => {\n      console.log(response.data);\n      this.post = response.data;\n      console.log(response.data);\n    }).catch(error => {\n      console.log(error);\n    });\n  }\n\n};","map":{"version":3,"mappings":"AAgDA,OAAOA,WAAP,MAAwB,kCAAxB;AACA,OAAOC,IAAP,MAAiB,2BAAjB;;AACA,MAAMC,KAAI,GAAIC,OAAO,CAAC,OAAD,CAArB;;AAEA,eAAe;AACXC,MAAI,EAAE,gBADK;AAEXC,YAAU,EAAE;AAAEL,eAAF;AAAeC;AAAf,GAFD;AAGXK,OAAK,EAAE,CAAC,IAAD,CAHI;;AAITC,MAAI,GAAG;AACL,WAAO;AACLC,UAAI,EAAE;AADD,KAAP;AAGD,GARQ;;AASTC,SAAO,GAAG;AACVP,SAAK,CAACQ,GAAN,CAAW,kCAAD,GAAqC,KAAKC,EAApD,EAAwD,EAAxD,EACGC,IADH,CACSC,QAAD,IAAc;AAClBC,aAAO,CAACC,GAAR,CAAYF,QAAQ,CAACN,IAArB;AACA,WAAKC,IAAL,GAAYK,QAAQ,CAACN,IAArB;AACAO,aAAO,CAACC,GAAR,CAAYF,QAAQ,CAACN,IAArB;AACD,KALH,EAMGS,KANH,CAMUC,KAAD,IAAW;AAChBH,aAAO,CAACC,GAAR,CAAYE,KAAZ;AACD,KARH;AASD;;AAnBU,CAAf","names":["CrudComment","Like","axios","require","name","components","props","data","post","mounted","get","id","then","response","console","log","catch","error"],"sourceRoot":"","sources":["/home/fanny/Rendu2/C-COD-280-PAR-2-2-dashboard-vladimira.grosjean/Dashboard2/src/views/CrudAdmin/CrudDetailPost.vue"],"sourcesContent":["<template>\n<section>\n      <div id=\"intro\" class=\"container w-50 p-5 text-center \"><!--v-if='login-admin' -->\n      <!-- <h1 class=\"mb-0 h4\"> {{post.title}}</h1> -->\n    </div>\n  <!--Main Navigation-->\n   \n    <div class=\"container\">\n      <!--Grid row-->\n            <h2>{{ post.title }}</h2>\n        <!--Grid column-->\n        <div style=\"width: 50%;\" class=\"container\">\n          <!--Section: Post data-mdb-->\n          <section class=\"border-bottom mb-4\">\n\n             <img :src=\"post.img_post\" class=\"img-fluid shadow-2-strong rounded-5 mb-4\" alt=\"image_post\">\n\n\n          </section>\n          <!--Section: Post data-mdb-->\n\n          <!--Section: Text-->\n          <section>\n            <p>\n             {{ post.content }}\n            </p>\n<div>\n            <Like />\n</div>\n\n          </section>\n          <!-- faire un v-for pour afficher tous les comments -->\n          <!--Section: Comments-->\n            <CrudComment :idPost=\"post.id\" />\n            \n          <!--Section: Reply-->\n        </div>\n        <!--Grid column-->\n\n        <!--Grid column-->\n      \n        <!--Grid column-->\n      <!--Grid row-->\n    </div>\n</section>\n</template>\n\n<script>\nimport CrudComment from '../../components/CrudComment.vue';\nimport Like from '../../components/Like.vue';\nconst axios = require('axios');\n\nexport default {\n    name: 'CrudDetailPost',\n    components: { CrudComment, Like},\n    props: ['id'],\n      data() {\n        return {\n          post: {},\n        };\n      },\n      mounted() {\n      axios.get(`http://localhost:8000/api/posts/` + this.id, {})\n        .then((response) => {\n          console.log(response.data);\n          this.post = response.data;\n          console.log(response.data)\n        })\n        .catch((error) => {\n          console.log(error);\n        })\n    },\n}\n\n\n\n</script>\n\n<style scoped>\n  #intro {\n        /* Margin to fix overlapping fixed navbar */\n        margin-top: 58px;\n      }\n\n      @media (max-width: 991px) {\n        #intro {\n          /* Margin to fix overlapping fixed navbar */\n          margin-top: 45px;\n        }\n      }\n</style>"]},"metadata":{},"sourceType":"module"}